        <PAGEOBJECT XPOS="723.628346456693" YPOS="16832.5937007874" OwnPage="38" ItemID="1499982471" PTYPE="4" WIDTH="510.2418898" HEIGHT="728.503937" FRTYPE="0" CLIPEDIT="0" PWIDTH="1" PLINEART="1" LOCALSCX="1" LOCALSCY="1" LOCALX="0" LOCALY="0" LOCALROT="0" PICART="1" SCALETYPE="1" RATIO="1" COLUMNS="2" COLGAP="14.17322835" AUTOTEXT="0" EXTRA="0" TEXTRA="0" BEXTRA="0" REXTRA="0" VAlign="0" FLOP="0" PLTSHOW="0" BASEOF="0" textPathType="0" textPathFlipped="0" path="M0 0 L510.242 0 L510.242 728.504 L0 728.504 L0 0 Z" copath="M0 0 L510.242 0 L510.242 728.504 L0 728.504 L0 0 Z" gXpos="0" gYpos="0" gWidth="0" gHeight="0" LAYER="1" NEXTITEM="1499972839" BACKITEM="1491972663">
