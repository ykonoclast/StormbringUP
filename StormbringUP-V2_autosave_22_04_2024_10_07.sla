                <para PARENT="puces"/>
                <ITEXT CPARENT="corps italique" CH="Coût"/>
                <nbspace CPARENT="corps italique"/>
                <ITEXT CPARENT="corps italique" CH=": "/>
                <ITEXT CH="1 point d’âme par année suivie"/>
                <para PARENT="puces"/>
                <para/>
                <ITEXT CPARENT="corps gras" CH="Laisser le temps vierge de sa souillure :"/>
                <para/>
                <ITEXT CH="Le sorcier enferme la fertilité d’une cible dans un miroir"/>
                <nbspace/>
                <ITEXT CH=": elle devient stérile, définitivement si le miroir est fracassé."/>
                <para/>
                <ITEXT CPARENT="corps italique" CH="Atelier"/>
                <nbspace CPARENT="corps italique"/>
                <ITEXT CPARENT="corps italique" CH=":"/>
                <ITEXT CH=" honorable"/>
                <para PARENT="puces"/>
                <ITEXT CPARENT="corps italique" CH="Temps de travail"/>
                <nbspace CPARENT="corps italique"/>
                <ITEXT CPARENT="corps italique" CH=":"/>
