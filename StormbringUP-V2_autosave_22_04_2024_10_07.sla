                <ITEXT CPARENT="corps italique" CH=": "/>
                <ITEXT CH="Dissolution"/>
                <para OpticalMargins="0"/>
                <ITEXT CPARENT="corps italique" CH="Ingrédients"/>
                <nbspace CPARENT="corps italique"/>
                <ITEXT CPARENT="corps italique" CH=":"/>
                <ITEXT CH=" craie pure de Vilmir, bourgeons de digitales et morceaux de sel passés par l’oesophage d’un mastodonte."/>
                <para/>
                <ITEXT CPARENT="corps italique" CH="Effet somatique"/>
                <nbspace CPARENT="corps italique"/>
                <ITEXT CPARENT="corps italique" CH=": "/>
                <ITEXT CH="rend inactifs les sens olfactifs et gustatifs pendant une heure par point d’âme dépensé."/>
                <para/>
                <ITEXT CPARENT="corps italique" CH="Effet toxique"/>
                <nbspace CPARENT="corps italique"/>
                <ITEXT CPARENT="corps italique" CH=":"/>
                <ITEXT CH=" un échec au test de virulence avec plus de dix points d’écart entraîne la perte définitive des sens impactés."/>
                <para/>
                <para/>
                <ITEXT CPARENT="corps gras" CH="Eau de déshydratation :"/>
                <para/>
                <ITEXT CPARENT="corps italique" CH="Virulence"/>
                <nbspace CPARENT="corps italique"/>
                <ITEXT CPARENT="corps italique" CH=": "/>
                <ITEXT CH="20"/>
                <para/>
                <ITEXT CPARENT="corps italique" CH="Runes"/>
                <nbspace CPARENT="corps italique"/>
                <ITEXT CPARENT="corps italique" CH=": "/>
                <ITEXT CH="Eau, Barrage"/>
                <para OpticalMargins="0"/>
                <ITEXT CPARENT="corps italique" CH="Ingrédients"/>
                <nbspace CPARENT="corps italique"/>
                <ITEXT CPARENT="corps italique" CH=": "/>
                <ITEXT CH="corps d’une ondine et encre empoisonnée de serpents de mer de Pan Tang."/>
                <para/>
                <ITEXT CPARENT="corps italique" CH="Effet somatique"/>
                <nbspace CPARENT="corps italique"/>
                <ITEXT CPARENT="corps italique" CH=":"/>
                <ITEXT CH=" un seul point d’âme rend le pourtour du corps du buveur étanche à tout liquide"/>
                <nbspace/>
                <ITEXT CH=": il peut même toucher de l’huile bouillante ou du poison car ceux-ci n’entreront pas en contact avec sa peau. Avec autant de point d’âme que le double de son physique, il peut même marcher sur l’eau."/>
                <para/>
                <ITEXT CPARENT="corps italique" CH="Effet toxique"/>
                <nbspace CPARENT="corps italique"/>
                <ITEXT CPARENT="corps italique" CH=":"/>
                <ITEXT CH=" trois doses dans la même journée et un échec au test de virulence empêchent le sujet de boire quoi que ce soit pendant un nombre de jours égal au nombre de points d’âme dépensés dans la concoction."/>
                <para/>
                <para/>
                <ITEXT CPARENT="corps gras" CH="Vin de Nihrain :"/>
                <para/>
                <ITEXT CPARENT="corps italique" CH="Virulence"/>
                <nbspace CPARENT="corps italique"/>
                <ITEXT CPARENT="corps italique" CH=":"/>
                <ITEXT CH=" 15"/>
                <para/>
                <ITEXT CPARENT="corps italique" CH="Runes"/>
                <nbspace CPARENT="corps italique"/>
                <ITEXT CPARENT="corps italique" CH=": "/>
                <ITEXT CH="Contresort"/>
                <para/>
                <ITEXT CPARENT="corps italique" CH="Ingrédients"/>
                <nbspace CPARENT="corps italique"/>
                <ITEXT CPARENT="corps italique" CH=": "/>
                <ITEXT CH="vin d’une vigne ayant été arrosée par le sang de nombreuses entités magiques."/>
                <para/>
                <ITEXT CPARENT="corps italique" CH="Effet somatique"/>
                <nbspace CPARENT="corps italique"/>
                <ITEXT CPARENT="corps italique" CH=": "/>
                <ITEXT CH="apaise et repose. Restaure les points d’âme (une bouteille en contient 100)."/>
                <para OpticalMargins="0"/>
                <ITEXT CPARENT="corps italique" CH="Effet toxique"/>
                <nbspace CPARENT="corps italique"/>
                <ITEXT CPARENT="corps italique" CH=":"/>
                <ITEXT CH=" équivalent à un alcool normal."/>
                <trail/>
            </StoryText>
