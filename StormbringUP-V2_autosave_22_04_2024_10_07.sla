                <para PARENT="corps tableau"/>
                <para PARENT="italique centré tableau"/>
                <ITEXT CH="Pouvoirs des élémentaires de la terre"/>
                <para PARENT="gras centré tableau"/>
                <ITEXT CPARENT="gras tableau" CH="Que la terre purifie ma nourriture"/>
                <nbspace CPARENT="gras tableau"/>
                <ITEXT CPARENT="gras tableau" CH=": "/>
                <ITEXT CH="l’élémentaire purifie un kilogramme de nourriture par RM. "/>
                <para PARENT="corps tableau"/>
                <para PARENT="corps tableau"/>
                <ITEXT CPARENT="gras tableau" CH="Que la puissance de la terre féconde ce lieu"/>
                <nbspace CPARENT="gras tableau"/>
                <ITEXT CPARENT="gras tableau" CH=":"/>
                <ITEXT CH=" l’élémentaire double la récolte dans un rayon de RM * 100m pour RM années."/>
                <para PARENT="corps tableau"/>
                <para PARENT="corps tableau"/>
                <ITEXT CPARENT="gras tableau" CH="Que la terre tremble pour abattre mes ennemis"/>
                <nbspace CPARENT="gras tableau"/>
                <ITEXT CPARENT="gras tableau" CH=":"/>
                <ITEXT CH=" l’élémentaire provoque un tremblement de terre dans un rayon de RM * 10m. Tous les personnages présents dans la zone doivent effectuer un jet de coordination moyen pour ne pas tomber et ont quoi qu’il arrive un malus de -5 à tous leurs jets physiques."/>
                <para PARENT="corps tableau"/>
                <para PARENT="corps tableau"/>
                <ITEXT CPARENT="gras tableau" CH="Que la puissance de la terre me protège"/>
                <nbspace CPARENT="gras tableau"/>
                <ITEXT CPARENT="gras tableau" CH=":"/>
