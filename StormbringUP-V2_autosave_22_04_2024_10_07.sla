        <PAGEOBJECT XPOS="723.628346456693" YPOS="25078.5751181103" OwnPage="56" ItemID="1491987719" PTYPE="4" WIDTH="510.2418898" HEIGHT="419.527559047236" FRTYPE="0" CLIPEDIT="0" PWIDTH="1" PLINEART="1" LOCALSCX="1" LOCALSCY="1" LOCALX="0" LOCALY="0" LOCALROT="0" PICART="1" SCALETYPE="1" RATIO="1" COLUMNS="2" COLGAP="14.17322835" AUTOTEXT="0" EXTRA="0" TEXTRA="0" BEXTRA="0" REXTRA="0" VAlign="0" FLOP="0" PLTSHOW="0" BASEOF="0" textPathType="0" textPathFlipped="0" path="M0 0 L510.242 0 L510.242 419.528 L0 419.528 L0 0 Z" copath="M0 0 L510.242 0 L510.242 419.528 L0 419.528 L0 0 Z" gXpos="0" gYpos="0" gWidth="0" gHeight="0" LAYER="1" NEXTITEM="1502114407" BACKITEM="1491868967"/>
        <PAGEOBJECT XPOS="984.415748031497" YPOS="23005.8198425197" OwnPage="52" ItemID="1491991047" PTYPE="4" WIDTH="249.4488189" HEIGHT="728.503937007885" FRTYPE="0" CLIPEDIT="1" PWIDTH="0.9921744099" PCOLOR="FromEPS#e6e6ff" PLINEART="1" TEXTFLOWMODE="2" LOCALSCX="1" LOCALSCY="1" LOCALX="0" LOCALY="0" LOCALROT="0" PICART="1" SCALETYPE="1" RATIO="1" COLUMNS="1" COLGAP="14.17322835" AUTOTEXT="0" EXTRA="5.669291339" TEXTRA="5.669291339" BEXTRA="5.669291339" REXTRA="5.669291339" VAlign="0" FLOP="0" PLTSHOW="0" BASEOF="0" textPathType="0" textPathFlipped="0" path="M0 0 L249.449 0 L249.449 728.504 L0 728.504 L0 0 Z" copath="M0 0 L249.448 0 L249.448 728.504 L0 728.504 L0 0 Z" gXpos="0" gYpos="0" gWidth="0" gHeight="0" LAYER="2" NEXTITEM="-1" BACKITEM="-1">
            <StoryText>
                <DefaultStyle/>
                <ITEXT CH="Capacités des automata"/>
                <para PARENT="Titre section"/>
                <ITEXT CH="Automata de combat"/>
                <para PARENT="gras centré tableau"/>
                <ITEXT CPARENT="gras tableau" CH="Absorption"/>
                <nbspace CPARENT="gras tableau"/>
                <ITEXT CPARENT="gras tableau" CH=": "/>
                <ITEXT CH="l’automaton absorbe la moitié des points d’âme utilisés contre lui. Tous les cinq points absorbés il augmente son trait le plus élevé d’un rang."/>
                <para PARENT="corps tableau"/>
                <para PARENT="corps tableau"/>
                <ITEXT CPARENT="gras tableau" CH="Bénédiction"/>
                <nbspace CPARENT="gras tableau"/>
                <ITEXT CPARENT="gras tableau" CH=":"/>
                <ITEXT CH=" l’automaton accorde un bonus de +10 à tous les jets d’attaque et a la défense de ses alliés."/>
                <para PARENT="corps tableau"/>
                <para PARENT="corps tableau"/>
                <ITEXT CPARENT="gras tableau" CH="Aura"/>
                <nbspace CPARENT="gras tableau"/>
                <ITEXT CPARENT="gras tableau" CH=":"/>
                <ITEXT CH=" l’automaton génère une aura de protection contre le Chaos"/>
                <nbspace/>
                <ITEXT CH=": tous les êtres marqués par le Chaos à moins de 5m de l’automaton subissent RM points de dégâts par phase."/>
                <para PARENT="corps tableau"/>
                <para PARENT="corps tableau"/>
                <ITEXT CH="Automata de voyage"/>
                <para PARENT="gras centré tableau"/>
                <ITEXT CPARENT="gras tableau" CH="Vitesse"/>
                <nbspace CPARENT="gras tableau"/>
                <ITEXT CPARENT="gras tableau" CH=":"/>
                <ITEXT CH=" une fois par jour l’automaton peut tripler sa vitesse pendant 5 tours de combat."/>
                <para PARENT="corps tableau"/>
                <para PARENT="corps tableau"/>
                <ITEXT CPARENT="gras tableau" CH="Air pur"/>
                <nbspace CPARENT="gras tableau"/>
                <ITEXT CPARENT="gras tableau" CH=":"/>
                <ITEXT CH=" l’automaton fournit de l’air respirable à ses passagers."/>
                <para PARENT="corps tableau"/>
                <para PARENT="corps tableau"/>
                <ITEXT CPARENT="gras tableau" CH="Invulnérabilité"/>
                <nbspace CPARENT="gras tableau"/>
                <ITEXT CPARENT="gras tableau" CH=": "/>
                <ITEXT CH="l’automaton est invulnérable aux armes humaines."/>
                <para PARENT="corps tableau"/>
                <para PARENT="gras centré tableau"/>
                <ITEXT CH="Automata de perception"/>
                <para PARENT="gras centré tableau"/>
                <ITEXT CPARENT="gras tableau" CH="Alarme"/>
                <nbspace CPARENT="gras tableau"/>
                <ITEXT CPARENT="gras tableau" CH=":"/>
                <ITEXT CH=" l’automaton ne parle pas, sa réponse est donnée par un changement de couleur, un son particulier ou tout autre phénomène binaire."/>
                <para PARENT="corps tableau"/>
                <para PARENT="corps tableau"/>
                <ITEXT CPARENT="gras tableau" CH="Réponse"/>
                <nbspace CPARENT="gras tableau"/>
                <ITEXT CPARENT="gras tableau" CH=":"/>
                <ITEXT CH=" l’automaton peut parler de manière simple et concise. Il peut par exemple traduire un texte sans poésie."/>
                <para PARENT="corps tableau"/>
                <para PARENT="corps tableau"/>
                <ITEXT CPARENT="gras tableau" CH="Dissertation"/>
                <nbspace CPARENT="gras tableau"/>
                <ITEXT CPARENT="gras tableau" CH=":"/>
                <ITEXT CH=" l’automaton est capable de dialoguer mais est plongé dans l’embarras quand les questions ne correspondent pas à sa logique absolue."/>
                <para PARENT="corps tableau"/>
                <para PARENT="corps tableau"/>
                <ITEXT CH="Automata réparateurs"/>
                <para PARENT="gras centré tableau"/>
                <ITEXT CPARENT="gras tableau" CH="Petite réparation"/>
                <nbspace CPARENT="gras tableau"/>
                <ITEXT CPARENT="gras tableau" CH=": "/>
                <ITEXT CH="l’automaton répare les dégâts mineurs de deux objets par jour"/>
                <para PARENT="corps tableau"/>
                <para PARENT="corps tableau"/>
                <ITEXT CPARENT="gras tableau" CH="Réparation"/>
                <nbspace CPARENT="gras tableau"/>
                <ITEXT CPARENT="gras tableau" CH=":"/>
                <ITEXT CH=" l’automaton répare des dégâts sérieux de quatre objets par jour y compris d’autres Automata (pas plus que médians)."/>
                <para PARENT="corps tableau"/>
                <para PARENT="corps tableau"/>
                <ITEXT CPARENT="gras tableau" CH="Rénovation"/>
                <nbspace CPARENT="gras tableau"/>
                <ITEXT CPARENT="gras tableau" CH=": "/>
                <ITEXT CH="l’automaton peut reconstruire un objet presque entièrement détruit et des réceptacles d’automata majeurs."/>
                <para PARENT="corps tableau"/>
                <para PARENT="corps tableau"/>
                <ITEXT CH="Automata medecins"/>
                <para PARENT="gras centré tableau"/>
                <ITEXT CPARENT="gras tableau" CH="Soins légers"/>
                <nbspace CPARENT="gras tableau"/>
                <ITEXT CPARENT="gras tableau" CH=":"/>
                <ITEXT CH=" l’automaton soigne les maladies jusqu’à une virulence 15 et restaure une blessure par jour."/>
                <para PARENT="corps tableau"/>
                <para PARENT="corps tableau"/>
                <ITEXT CPARENT="gras tableau" CH="Soins moyens"/>
                <nbspace CPARENT="gras tableau"/>
                <ITEXT CPARENT="gras tableau" CH=":"/>
                <ITEXT CH=" l’automaton restaure deux blessures par jour et soigne toutes les maladies non magiques."/>
                <para PARENT="corps tableau"/>
                <para PARENT="corps tableau"/>
                <ITEXT CPARENT="gras tableau" CH="Soins avancés"/>
                <nbspace CPARENT="gras tableau"/>
                <ITEXT CPARENT="gras tableau" CH=":"/>
                <ITEXT CH=" l’automaton peut créer des prothèses (une fois par invocation) pouvant remplacer de façon rudimentaire un membre. Il restaure 4 blessures par jour mais ne peut le faire sur un individu d’alignement chaotique."/>
                <para PARENT="corps tableau"/>
                <para PARENT="gras centré tableau"/>
                <ITEXT CH="Automata de fabrication"/>
                <para PARENT="gras centré tableau"/>
                <ITEXT CPARENT="gras tableau" CH="Expertise"/>
                <nbspace CPARENT="gras tableau"/>
                <ITEXT CPARENT="gras tableau" CH=":"/>
                <ITEXT CH=" l’automaton ne fait aucune erreur"/>
                <nbspace/>
                <ITEXT CH=": il distingue toujours le bon matériau du mauvais et sait repérer les fausses pierres précieuses par exemple."/>
                <para PARENT="corps tableau"/>
                <para PARENT="corps tableau"/>
                <ITEXT CPARENT="gras tableau" CH="Rapidité"/>
                <nbspace CPARENT="gras tableau"/>
                <ITEXT CPARENT="gras tableau" CH=": "/>
                <ITEXT CH="l’automaton met deux fois moins de temps qu’il n’en faudrait à un être humain pour la tâche à accomplir."/>
                <para PARENT="corps tableau"/>
                <para/>
                <ITEXT CPARENT="gras tableau" CH="Chef d’oeuvre"/>
                <nbspace CPARENT="gras tableau"/>
                <ITEXT CPARENT="gras tableau" CH=": "/>
                <ITEXT CH="l’objet ou le bâtiment créé est d’une qualité inhumaine."/>
                <para PARENT="corps tableau"/>
            </StoryText>
            <PageItemAttributes>
                <ItemAttribute Name="TOC" Type="" Value="Capacités des automata" Parameter="" Relationship="none" RelationshipTo="" AutoAddTo="none"/>
            </PageItemAttributes>
        </PAGEOBJECT>
