                <para PARENT="corps tableau"/>
                <ITEXT CPARENT="gras tableau" CH="Allure"/>
                <nbspace CPARENT="gras tableau"/>
                <ITEXT CPARENT="gras tableau" CH=":"/>
                <ITEXT FONTSIZE="8" CH=" le démon possède un bonus de +15 à tous ses jets sociaux"/>
                <ITEXT CH="."/>
                <para PARENT="corps tableau"/>
                <para PARENT="corps tableau"/>
                <ITEXT CPARENT="gras tableau" CH="Antennes"/>
                <nbspace CPARENT="gras tableau"/>
                <ITEXT CPARENT="gras tableau" CH=": "/>
                <ITEXT FONTSIZE="8" CH="le démon peut percevoir soit"/>
                <nbspace FONTSIZE="8"/>
                <ITEXT FONTSIZE="8" CH=": les volumes, la chaleur, la folie, les êtres vivants ou les manifestations magiques"/>
                <ITEXT CH="."/>
                <para PARENT="corps tableau"/>
                <para PARENT="corps tableau"/>
                <ITEXT CPARENT="gras tableau" CH="Aquatique"/>
                <nbspace CPARENT="gras tableau"/>
                <ITEXT CPARENT="gras tableau" CH=":"/>
                <ITEXT FONTSIZE="8" CH=" le démon possède des extrêmités palmées pour se mouvoir rapidement dans l’eau."/>
                <para PARENT="corps tableau"/>
                <para PARENT="corps tableau"/>
                <ITEXT CPARENT="gras tableau" CH="Arbalète"/>
                <nbspace CPARENT="gras tableau"/>
                <ITEXT CPARENT="gras tableau" CH=":"/>
                <ITEXT FONTSIZE="8" CH=" le démon peut projeter des épines osseuses (il en produit 5 par heure) qui possèdent une VD de 20."/>
                <para PARENT="corps tableau"/>
                <para PARENT="corps tableau"/>
                <ITEXT CPARENT="gras tableau" CH="Arme spécifique"/>
                <nbspace CPARENT="gras tableau"/>
                <ITEXT CPARENT="gras tableau" CH=":"/>
                <ITEXT FONTSIZE="8" CH=" le démon possède une arme infligeant 10 points de dégâts de plus à chaque touche"/>
                <ITEXT CH="."/>
                <para PARENT="corps tableau"/>
                <para PARENT="corps tableau"/>
                <ITEXT CPARENT="gras tableau" CH="Armure naturelle"/>
                <nbspace CPARENT="gras tableau"/>
                <ITEXT CPARENT="gras tableau" CH=":"/>
                <ITEXT FONTSIZE="8" CH=" le démon possède une armure naturelle de 5 points d’armure."/>
                <para PARENT="corps tableau"/>
                <para PARENT="corps tableau"/>
                <ITEXT CPARENT="gras tableau" CH="Armure spécifique"/>
                <nbspace CPARENT="gras tableau"/>
                <ITEXT CPARENT="gras tableau" CH=":"/>
                <ITEXT FONTSIZE="8" CH=" le démon possède une armure dont la valeur est majorée de 5 points d’armure"/>
                <ITEXT CH="."/>
                <para PARENT="corps tableau"/>
                <para PARENT="corps tableau"/>
                <ITEXT CPARENT="gras tableau" CH="Arrêt du temps (réservé aux démons majeurs)"/>
                <nbspace CPARENT="gras tableau"/>
                <ITEXT CPARENT="gras tableau" CH=":"/>
                <ITEXT FONTSIZE="8" CH=" le démon stoppe l’écoulement du temps pendant un nombre de phases égal à son mental. Les personnages réussissant un jet de volonté très difficile peuvent agir."/>
                <para PARENT="corps tableau"/>
                <para PARENT="corps tableau"/>
                <ITEXT CPARENT="gras tableau" CH="Artiste surnaturel"/>
                <nbspace CPARENT="gras tableau"/>
                <ITEXT CPARENT="gras tableau" CH=":"/>
                <ITEXT FONTSIZE="8" CH=" le démon produit des œuvres ayant un rang de peur de 3 mais dont l’effet est de fasciner plus que de terroriser. La victime ne peut s’en détacher."/>
                <para PARENT="corps tableau"/>
                <para PARENT="corps tableau"/>
                <ITEXT CPARENT="gras tableau" CH="Asymétrie"/>
                <nbspace CPARENT="gras tableau"/>
                <ITEXT CPARENT="gras tableau" CH=":"/>
                <ITEXT FONTSIZE="8" CH=" le démon possède un bonus de 10 à son ND à sa défense."/>
                <para PARENT="corps tableau"/>
                <para PARENT="corps tableau"/>
                <ITEXT CPARENT="gras tableau" CH="Beauté"/>
                <nbspace CPARENT="gras tableau"/>
                <ITEXT CPARENT="gras tableau" CH=":"/>
                <ITEXT FONTSIZE="8" CH=" le démon possède un rang de plus en sociabilité."/>
                <para PARENT="corps tableau"/>
                <para PARENT="corps tableau"/>
                <ITEXT CPARENT="gras tableau" CH="Bras supplémentaire"/>
                <nbspace CPARENT="gras tableau"/>
                <ITEXT CPARENT="gras tableau" CH=":"/>
                <ITEXT FONTSIZE="8" CH=" le démon possède une "/>
                <ITEXT CH="attaque supplémentaire par tour (uniquement avec une seule arme à une main)."/>
                <para PARENT="corps tableau"/>
                <para PARENT="corps tableau"/>
                <ITEXT CPARENT="gras tableau" CH="Brûlure"/>
                <nbspace CPARENT="gras tableau"/>
                <ITEXT CPARENT="gras tableau" CH="ou glace :"/>
                <ITEXT FONTSIZE="8" CH=" quiconque approche le démon subit RM points de dégâts par phase car le démon est brûlant ou glacé. Seul le froid ou la chaleur magique permet de s’en prémunir."/>
                <para PARENT="corps tableau"/>
                <para PARENT="corps tableau"/>
                <ITEXT CPARENT="gras tableau" CH="Brume"/>
                <nbspace CPARENT="gras tableau"/>
                <ITEXT CPARENT="gras tableau" CH=":"/>
                <ITEXT FONTSIZE="8" CH=" les assaillants doivent réussir un jet de perception moyen pour frapper le "/>
                <ITEXT CH="démon."/>
                <para PARENT="corps tableau"/>
                <para PARENT="corps tableau"/>
                <ITEXT CPARENT="gras tableau" CH="Caméléon"/>
                <nbspace CPARENT="gras tableau"/>
                <ITEXT CPARENT="gras tableau" CH=": "/>
                <ITEXT FONTSIZE="8" CH="le démon possède un bonus de +10 à tous ses jets de furtivité."/>
                <para PARENT="corps tableau"/>
                <para PARENT="corps tableau"/>
                <ITEXT CPARENT="gras tableau" CH="Clairvoyance"/>
                <nbspace CPARENT="gras tableau"/>
                <ITEXT CPARENT="gras tableau" CH=":"/>
                <ITEXT FONTSIZE="8" CH=" le démon possède un rang supplémentaire en mental."/>
                <para PARENT="corps tableau"/>
                <para PARENT="corps tableau"/>
                <ITEXT CPARENT="gras tableau" CH="Compétent"/>
                <nbspace CPARENT="gras tableau"/>
                <ITEXT CPARENT="gras tableau" CH=":"/>
                <ITEXT FONTSIZE="8" CH=" le démon possède deux compétences un rang au dessus de la normale "/>
                <ITEXT CH="(déterminée par son RM)."/>
                <para PARENT="corps tableau"/>
                <para PARENT="corps tableau"/>
                <ITEXT CPARENT="gras tableau" CH="Constricteur"/>
                <nbspace CPARENT="gras tableau"/>
                <ITEXT CPARENT="gras tableau" CH=": "/>
                <ITEXT FONTSIZE="8" CH="si le démon réussit son attaque, il emprisonne sa victime dans ses anneaux et lui inflige 5 points de dégâts par phase. La victime peut tenter de s’échapper."/>
                <para PARENT="corps tableau"/>
                <para PARENT="corps tableau"/>
                <ITEXT CPARENT="gras tableau" CH="Contagion"/>
                <nbspace CPARENT="gras tableau"/>
                <ITEXT CPARENT="gras tableau" CH=":"/>
                <ITEXT FONTSIZE="8" CH=" le démon peut infliger une maladie de virulence 15 causant fièvres, amaigrissement et démence ainsi que l'encaissement de 5 points de dégâts par jour et la perte d’un rang de mental tous les deux jours. Quand le mental tombe à 0 la victime devient folle et la maladie prend fin."/>
                <para PARENT="corps tableau"/>
                <para PARENT="corps tableau"/>
