                <para OpticalMargins="0"/>
                <ITEXT CPARENT="corps italique" CH="Effet somatique"/>
                <nbspace CPARENT="corps italique"/>
                <ITEXT CPARENT="corps italique" CH=": "/>
                <ITEXT CH="la cible croit tout ce que le porteur de la fragrance dit, les effets se dissipent dans les minutes suivant l’éloignement."/>
                <para/>
                <ITEXT CPARENT="corps italique" CH="Effet toxique"/>
                <nbspace CPARENT="corps italique"/>
                <ITEXT CPARENT="corps italique" CH=": "/>
                <ITEXT CH="un échec au test de virulence avec plus de dix points d'écart entraîne une addiction forte avec délai d’une journée."/>
                <para/>
                <para/>
                <ITEXT CPARENT="corps gras" CH="Pilule d’imperception :"/>
                <para/>
                <ITEXT CPARENT="corps italique" CH="Virulence"/>
