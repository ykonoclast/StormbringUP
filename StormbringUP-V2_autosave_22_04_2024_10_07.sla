                <nbspace CPARENT="corps italique"/>
                <ITEXT CPARENT="corps italique" CH=":"/>
                <ITEXT CH=" 10 points d’âme par semaine de cauchemars"/>
                <para PARENT="puces"/>
                <ITEXT CH="Les sorts médians de l’école du firmament"/>
                <para PARENT="développement section"/>
                <ITEXT CPARENT="corps gras" CH="Les mystères d'une vie :"/>
                <para/>
                <ITEXT CH="Le sorcier étudie la vie d’un individu comme si il en avait été témoin."/>
                <para/>
                <ITEXT CPARENT="corps italique" CH="Atelier"/>
                <nbspace CPARENT="corps italique"/>
                <ITEXT CPARENT="corps italique" CH=": "/>
                <ITEXT CH="superbe"/>
                <para PARENT="puces"/>
                <ITEXT CPARENT="corps italique" CH="Temps de travail"/>
                <nbspace CPARENT="corps italique"/>
                <ITEXT CPARENT="corps italique" CH=":"/>
