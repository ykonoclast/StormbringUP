            <StoryText>
                <DefaultStyle/>
                <ITEXT CH="Les traits démoniaques"/>
                <para PARENT="Titre section"/>
                <ITEXT CPARENT="gras tableau" CH="Absorption"/>
                <nbspace CPARENT="gras tableau"/>
                <ITEXT CPARENT="gras tableau" CH=": "/>
                <ITEXT FONTSIZE="8" CH="le démon absorbe la moitié des points d’âme utilisés contre lui. Tous les cinq points absorbés il augmente son trait le plus élevé d’un rang."/>
                <para PARENT="corps tableau"/>
                <para PARENT="corps tableau"/>
                <ITEXT CPARENT="gras tableau" CH="Acidité"/>
                <nbspace CPARENT="gras tableau"/>
                <ITEXT CPARENT="gras tableau" CH=":"/>
                <ITEXT FONTSIZE="8" CH=" le sang acide du démon inflige 10 points de dégâts à tous ceux qui lui infligent des dégâts"/>
                <ITEXT CH="."/>
                <para PARENT="corps tableau"/>
                <para PARENT="corps tableau"/>
                <ITEXT CPARENT="gras tableau" CH="Adresse"/>
                <nbspace CPARENT="gras tableau"/>
                <ITEXT CPARENT="gras tableau" CH=":"/>
                <ITEXT FONTSIZE="8" CH=" le démon possède un rang de plus en coordination"/>
